.main-container {
  background: #f0f0f0;
  width: 100%;
  height: 100%; }

.main-header {
  width: 100%;
  height: 150px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: url(/images/user_head_bg.jpg); }

.denglu {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center; }

.icondenglu {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  background: url(/images/icon_head_default.png);
  background-position: 50% 50%;
  background-size: 100%;
  background-repeat: no-repeat; }

.textdenglu {
  font-size: 12px;
  color: white; }

.main-list {
  width: 100%;
  height: 90px;
  background: #fff; }

.main-list ul {
  width: 100%;
  height: 90px;
  display: flex; }

.main-list ul li {
  flex: 1;
  height: 100%;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center; }

.main-list ul li view {
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center; }

.main-list ul li.li3 view {
  height: 50px;
  width: 100%;
  border-right: 1px solid #e1e1e1;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center; }

.main-list ul li image {
  width: 20px;
  height: 20px;
  margin: auto; }

.main-list ul li b {
  font-size: 12px;
  color: #666;
  display: block;
  margin-top: 10px; }

