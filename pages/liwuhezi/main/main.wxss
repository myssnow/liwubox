.main-list{
  height: 100%;
  background-color:#f0f0f0;
}
.main-list scroll-view{
  height: 100%;
}
.main-list scroll-view button{
  font-size: 14px;
  border: 0;
}
.main-list scroll-view ul{
  display: flex;
  flex-direction: column;
  /*padding-right: 8px;*/
}
.main-list scroll-view ul navigator{

  padding: 10px;
  box-sizing: border-box;
  background-color: white;
  margin-top: 10px;
}
.main-list scroll-view ul navigator li{
  width: 100%;
  height: 220px;
  background-color: white;

}
.apply{
  width: 100%;
  height: 220px;
  position: relative;
  display: flex;
  flex-direction: column;
  border:solid 1px #e6e6e6;
}
.apply image{
  width: 100%;
  height: 85%;
}
.shiyi{
  display: flex;
  justify-content: space-between;
  flex: 1;
  align-items: center;
}
.shiyileft{
  display: flex;
  justify-content: space-around;
 align-items: center;
}
.shiyileft span{
  font-size: .2rem;
  color: #969696;
}
.shiyileft view{
  width: 3rem;
  height: 1rem;
  display: block;
  font-size: .1rem;
  border-radius:25px;
  line-height: 1rem;
  text-align: center;
  margin-right: 0.5rem;
  border: solid 1px #77a77e;
  color: #77a77e;
}
.love{
  font-size: .1rem;
  color: #808080;
}
.title{
  position: absolute;
  width: 60%;
  height: 35%;
  border: solid 1px rgba(255, 255, 255, 0.5);
  top: 25%;
  left: 15%;
  padding: 2%;
}
.dl{
  display: block;
  width:100%;
  height: 100%;
  background:rgba(255, 255, 255, 0.8);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.top{
  font-size:1.5rem;
  color: #2d3f4f;
  /*text-align: center;*/
}
.bottom{
  font-size: .1rem;
  color: #bcb7b3;
}
